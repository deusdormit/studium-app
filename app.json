    "web": {
      "favicon": "./assets/favicon.png"
    },
    "plugins": [
      [
        "expo-font",
        {
          "fonts": [
            "./assets/fonts/Aspekta-400.ttf",
            "./assets/fonts/Aspekta-450.ttf",
            "./assets/fonts/Aspekta-500.ttf",
            "./assets/fonts/Aspekta-550.ttf",
            "./assets/fonts/Aspekta-600.ttf"
          ]
        }
      ]
    ]
